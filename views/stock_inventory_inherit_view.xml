<?xml version="1.0" encoding="utf-8"?>
<odoo>

		<record model="ir.ui.view" id="stock_inventory_inherit_form_view">
				<field name="name">stock.inventory.inherit.form</field>
				<field name="model">stock.inventory</field>
				<field name="inherit_id" ref="stock.view_inventory_form" />
				<field name="arch" type="xml">

					<xpath expr="//field[@name='company_id']" position="after">
						<field name="excel_file"/>
						<button name="button_update_lines" string="Actualizar" type="object" class="btn-primary" attrs="{'invisible': [('excel_file', '=', False)]}"/>
					</xpath>

				</field>
		</record>


</odoo>